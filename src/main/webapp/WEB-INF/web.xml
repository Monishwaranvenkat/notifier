<?xml version="1.0" encoding="UTF-8"?>

<web-app xmlns="http://java.sun.com/xml/ns/javaee"
         version="2.5"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
    
	<welcome-file-list>
    <welcome-file>login.jsp</welcome-file>
 </welcome-file-list>



    
    
    <!-- signup page -->
    
    <servlet>
        <servlet-name>SignupServlet</servlet-name>
        <jsp-file>/signup.jsp</jsp-file>
    </servlet>
    <!--mapping-->
    <servlet-mapping>
        <servlet-name>SignupServlet</servlet-name>
        <url-pattern>/signup</url-pattern>
    </servlet-mapping>
    
    <servlet>
        <servlet-name>loginServlet</servlet-name>
         <servlet-class> com.example.demo.servlet.LoginServlet</servlet-class>
    </servlet>
    <!--mapping-->
    <servlet-mapping>
        <servlet-name>loginServlet</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    
    
      <servlet>
        <servlet-name>logoutServlet</servlet-name>
         <servlet-class> com.example.demo.servlet.LogoutServlet</servlet-class>
    </servlet>
    <!--mapping-->
    <servlet-mapping>
        <servlet-name>logoutServlet</servlet-name>
        <url-pattern>/logout</url-pattern>
    </servlet-mapping>
    
    
        <servlet>
        <servlet-name>notebookServlet</servlet-name>
         <servlet-class> com.example.demo.servlet.NoteBookServlet</servlet-class>
    </servlet>
    <!--mapping-->
    <servlet-mapping>
        <servlet-name>notebookServlet</servlet-name>
        <url-pattern>/notebook</url-pattern>
    </servlet-mapping>
    
    
    
    <servlet>
   <servlet-name>test</servlet-name>
